#include <stdio.h>
#include <stdlib.h>
#define NUMEROS 5

int main(void) {
	setbuf(stdout, NULL);

	int arrayNum [NUMEROS] = {9, 15, 18, 21, 5};


	return EXIT_SUCCESS;
}

int ordenarArray (int* pArray, int limite)
{
	int i;
	int numMin;
	int auxiliar;
	int indiceMin;
	int nuevoLimite = limite-1;

	if (pArray != NULL && limite >= 0)
	{
		for (i=0; i<nuevoLimite; i++)
		{
			numMin = pArray[i];

			if (i != indiceMin)
			{
				if (pArray [i] < numMin)
				{
					numMin = pArray[i];
				}
				auxiliar = pArray [i];
				numMin = pArray [i];


			}
		}
	}

}
