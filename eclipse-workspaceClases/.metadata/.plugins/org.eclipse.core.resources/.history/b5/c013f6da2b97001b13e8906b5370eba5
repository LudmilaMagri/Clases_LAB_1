#include <stdio.h>
#include <stdlib.h>
#define NUMEROS 5

int main(void) {
	setbuf(stdout, NULL);

	int arrayNum [NUMEROS] = {9, 15, 18, 21, 5};


	return EXIT_SUCCESS;
}

int ordenarArray (int* pArray, int* pArrayMin, int limite)
{
	int retorno = -1;
	int i;
	int numMin;
	int auxiliar;
	int flag = 0;
	int nuevoLimite = limite-1;

	if (pArray != NULL && limite >= 0)
	{
		numMin = pArrayMin[i];

		for (i=0; i<nuevoLimite; i++)
		{
			if (i != pArrayMin)
			{
				if (pArrayMin [i] < numMin)
				{
					numMin = pArrayMin[i];
				}

				pArray[i]= auxiliar;
				numMin = pArray [i];
				auxiliar = pArrayMin [i];
			}
		}
		retorno =0;
	}
	return retorno;
}
